{
  "version": 2,
  "name": "capitalcue",
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/next",
      "config": {
        "projectSettings": {
          "framework": "nextjs"
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/web/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "@api_url",
    "NEXT_PUBLIC_APP_NAME": "CapitalCue"
  },
  "functions": {
    "apps/web/pages/api/**/*.js": {
      "maxDuration": 30
    }
  }
}